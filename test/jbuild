(jbuild_version 1)

(executables
 ((names (test test_code test_core test_crypto test_data test_ffi test_geo test_io test_lang test_math test_media test_meta test_net test_qty test_std test_sys test_text test_time test_util))
  (libraries (DRY))))

(alias
 ((name runtest)
  (deps ((glob_files test*.exe)))
  (action (run ./jbuild_run.sh ${^}))))
